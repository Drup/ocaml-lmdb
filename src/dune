(library
 (name lmdb)
 (public_name lmdb)
 (synopsis "Bindings for LMDB, a fast in-file database with ACID transactions")
 (flags :standard -safe-string -thread)
 (libraries bigstringaf)
 (c_flags :standard -Wall -Wextra -Wno-unused-parameter -pedantic -std=c99)
 (c_names lmdb_stubs)
 (c_library_flags -llmdb)
)
